    BackgroundColor="{DynamicResource BackgroundColor}">

    <WebView x:Name="newsPresenter">
        <WebView.Source>
            <UrlWebViewSource Url="{Binding Url}"/>
        </WebView.Source>
    </WebView>

</ContentPage>
